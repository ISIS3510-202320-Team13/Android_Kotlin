<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.viewModel.view.Recibos">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView36"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Recibo de pago"
            android:textColor="@color/mainBlue"
            app:layout_constraintBottom_toTopOf="@+id/textView38"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.303" />

        <TextView
            android:id="@+id/textView37"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:layout_marginBottom="11dp"
            android:text="PARKING APP"
            android:textColor="#2597FA"
            android:textSize="24sp"
            app:layout_constraintBottom_toTopOf="@+id/editTextText7"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.108"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView36" />

        <TextView
            android:id="@+id/editTextText7"
            android:layout_width="0dp"

            android:layout_height="80dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="108dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/text_background"
            android:ems="10"
            android:gravity="center_vertical|center_horizontal"
            android:inputType="text"
            android:text="180 min - $13.000"
            android:textAlignment="center"
            android:textColor="@color/Main_Blue"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/editTextText9"
            android:layout_width="0dp"
            android:layout_height="80dp"
            android:gravity="center_vertical|center_horizontal"

            android:layout_marginStart="20dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/text_background"
            android:ems="10"
            android:inputType="text"
            android:text="Diciembre 6 del 2023"
            android:textAlignment="center"
            android:textColor="@color/Main_Blue"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextText7" />

        <TextView
            android:id="@+id/editTextText10"
            android:layout_width="0dp"
            android:layout_height="80dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="30dp"
            android:gravity="center_vertical|center_horizontal"

            android:layout_marginEnd="20dp"
            android:background="@drawable/text_background"
            android:ems="10"
            android:inputType="text"
            android:text="City Parking - Uniandes"
            android:textAlignment="center"
            android:textColor="@color/Main_Blue"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextText9" />

        <TextView
            android:id="@+id/editTextText11"
            android:layout_width="0dp"
            android:layout_height="80dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="20dp"
            android:gravity="center_vertical|center_horizontal"

            android:background="@drawable/text_background"
            android:ems="10"
            android:inputType="text"
            android:text="YR3578SDT321DG34T"
            android:textAlignment="center"
            android:textColor="@color/Main_Blue"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextText10" />

        <TextView
            android:id="@+id/textView38"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:text="Tiempo y valor pagado"
            android:textColor="@color/mainBlue"
            android:textSize="12sp"
            app:layout_constraintStart_toStartOf="@+id/editTextText7"
            app:layout_constraintTop_toTopOf="@+id/editTextText7" />

        <TextView
            android:id="@+id/textView39"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:text="Fecha"
            android:textColor="@color/mainBlue"
            android:textSize="12sp"
            app:layout_constraintStart_toStartOf="@+id/editTextText9"
            app:layout_constraintTop_toTopOf="@+id/editTextText9" />

        <TextView
            android:id="@+id/textView40"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:text="Lugar"
            android:textColor="@color/mainBlue"
            android:textSize="12sp"
            app:layout_constraintStart_toStartOf="@+id/editTextText10"
            app:layout_constraintTop_toTopOf="@+id/editTextText10" />

        <TextView
            android:id="@+id/textView41"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:text="Numero de reserva"
            android:textColor="@color/mainBlue"
            android:textSize="12sp"
            app:layout_constraintStart_toStartOf="@+id/editTextText11"
            app:layout_constraintTop_toTopOf="@+id/editTextText11" />

        <TextView
            android:id="@+id/textView42"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:layout_marginTop="20dp"
            android:text="Recomendaciones"
            android:textColor="@color/mainBlue"
            android:textSize="12sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextText11" />

        <TextView
            android:id="@+id/textView44"
            android:layout_width="409dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="20dp"
            android:elegantTextHeight="false"
            android:text="Recuerda que al comprar tiempo estás pagando por adelantado el precio total de tu tarifa de parqueo, sin embargo, el tiempo que reservaste es el que con el espacio asignado"
            android:textAlignment="center"
            android:textAllCaps="false"
            android:textColor="@color/mainBlue"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.526"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView42" />

        <TextView
            android:id="@+id/textView45"
            android:layout_width="409dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="20dp"
            android:elegantTextHeight="false"
            android:text="En caso de demorarte más de lo comprado informa al encargado y esta persona indicará si es posible dejar el vehiculo o no, de ser posible se te cobrará por el tiempo extra de no tener tarifa plena"
            android:textAlignment="center"
            android:textAllCaps="false"
            android:textColor="@color/mainBlue"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.526"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView44" />

        <TextView
            android:id="@+id/textView46"
            android:layout_width="409dp"
            android:layout_height="34dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="20dp"
            android:text="De no dar previo aviso se podrá retirar el vehiculo de su espacio y se cobrará una multa establecida por el parqueadero"
            android:textColor="@color/mainBlue"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView45" />

        <Button
            android:id="@+id/button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:text="Aceptar"
            android:textColor="@color/white"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView46" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>